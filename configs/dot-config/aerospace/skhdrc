lalt - q: aerospace workspace 1 > /dev/null 2>&1
lalt - w: aerospace workspace 2 > /dev/null 2>&1
lalt - e: aerospace workspace 3 > /dev/null 2>&1
lalt - r: aerospace workspace 4 > /dev/null 2>&1
lalt - t: aerospace workspace 5 > /dev/null 2>&1

lalt + shift - q: aerospace move-node-to-workspace 1 --focus-follows-window > /dev/null 2>&1
lalt + shift - w: aerospace move-node-to-workspace 2 --focus-follows-window > /dev/null 2>&1
lalt + shift - e: aerospace move-node-to-workspace 3 --focus-follows-window > /dev/null 2>&1
lalt + shift - r: aerospace move-node-to-workspace 5 --focus-follows-window > /dev/null 2>&1
lalt + shift - t: aerospace move-node-to-workspace 5 --focus-follows-window > /dev/null 2>&1
