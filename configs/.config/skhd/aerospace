meh - a: aerospace workspace 1 > /dev/null 2>&1
meh - s: aerospace workspace 2 > /dev/null 2>&1
meh - d: aerospace workspace 3 > /dev/null 2>&1
meh - f: aerospace workspace 4 > /dev/null 2>&1
meh - g: aerospace workspace 5 > /dev/null 2>&1

hyper - a: aerospace move-node-to-workspace 1 --focus-follows-window > /dev/null 2>&1
hyper - s: aerospace move-node-to-workspace 2 --focus-follows-window > /dev/null 2>&1
hyper - d: aerospace move-node-to-workspace 3 --focus-follows-window > /dev/null 2>&1
hyper - f: aerospace move-node-to-workspace 4 --focus-follows-window > /dev/null 2>&1
hyper - g: aerospace move-node-to-workspace 5 --focus-follows-window > /dev/null 2>&1

lctrl - h: aerospace focus left
lctrl - j: aerospace focus down
lctrl - k: aerospace focus up
lctrl - l: aerospace focus right

